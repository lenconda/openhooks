{
  "rules": {
    "class-name": true,
    "member-access": [true, "no-public"],
    "no-any": false,
    "no-non-null-assertion": true,
    "no-var-requires": false,
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ],
    "prefer-for-of": true,
    "typedef": [false, "call-signature", "parameter", "member-variable-declaration"],
    "eofline": true,
    "indent": [true, "spaces"],
    "member-ordering": [false],
    "no-eval": true,
    "no-sparse-arrays": true,
    "switch-default": true,
    "prefer-const": true,
    "arrow-return-shorthand": true,
    "trailing-comma": [
      false,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": false,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "variable-name": false,
    "whitespace": [
      true
    ]
  }
}
